<p id="notice"><%= notice %></p>

<h1>
  <%= @scan.name %>
</h1>

<p>
  Found Subdomains: <%= @scan.subdomain_records.length %><hr>
  <% @subdomain_records.each do |r| %>
    <%= r.name %><br>
  <% end %>
</p>

<%= will_paginate @subdomain_records %>
<%= link_to 'Export to CSV', scan_path(@scan, format: :csv), class: "button tiny" %>
<%= render 'comparison_form' %>
<%= link_to 'Back to Scan Search', scans_path, class: 'button tiny' %>
